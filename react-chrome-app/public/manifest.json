{
    "name": "Download Image",
    "description": "Download Image",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "index.html",
        "default_title": "Download Image"
    },
    "content_scripts": [
        {
            "matches": ["http://*/*", "https://*/*"],
            "js": ["./static/js/content.js"]
        }
    ],
    "permissions": ["activeTab", "tabs"],
    "icons": {
        "16": "logo192.png",
        "48": "logo192.png",
        "128": "logo192.png"
    }
}
